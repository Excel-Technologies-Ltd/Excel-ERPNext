  "html": "<div id=\"header-html\" class=\"hidden-pdf row section-break\">\n    <div class=\"letter-head\">\n        <div><img src=\"https://bd.bazrabd.org/files/ETL-header.jpg\" style=\"width: 861px;\">\n            <br>\n        </div>\n    </div>\t\t    \n    <div class=\"print-heading\" style=\"padding-bottom: 6px !important; margin-bottom: 10px;\">\n        <div class=\"2nd-head\">\n            <p style=\"display: inline-block; text-align: left; width: 33%; \"><b> {{ doc.name }}</b></p>\n               \n                <p style=\"display: inline-block; text-align: center; width: 32%; \"><b> Quotation </b></p>\n            \n            <p style=\"display: inline-block; text-align: right; width: 33%;\"><b> {{ doc.territory }}</b></p>\n        </div> \n    </div>\n</div>\n\n\n    <div class=\"row section-break\" style=\"margin: 0px !important\">\n        <table class=\"col-xs-5\">\n            \n        \n        <tr>\t\n                <td style=\"padding: 0px !important;\" class=\"text-left\"><b>Customer</td>\n                <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n                <!--<td style=\"padding: 0px !important;\"><b>{{ doc.get_formatted(\"customer_name\") }}</td>-->\n            {%- if doc.customer_name == 'EXCEL EXTERNAL CUSTOMER' -%}\n<td style=\"padding: 0px !important;\"><b>{{ doc.get_formatted(\"custom_external_customer\") }}</b></td>\n{%- else -%}\n<td style=\"padding: 0px !important;\"><b>{{ doc.get_formatted(\"customer_name\") }}</b></td>\n{%- endif -%}\n\n        </tr>\t \n        <tr>\t\n                <td style=\"padding: 0px !important;\" class=\"text-left\"> <b>Address</b></td>\n                <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n            {%- if doc.customer_name == 'EXCEL EXTERNAL CUSTOMER' -%}\n<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"custom_address\") }}</td>\n{%- else -%}\n<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"address_display\") }}</td>\n{%- endif -%}\n\n        </tr>\n        <tr>\t\n                <td style=\"padding: 0px !important;\" class=\"text-left\"> <b>Contact</b></td>\n                <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n             {%- if doc.customer_name == 'EXCEL EXTERNAL CUSTOMER' -%}\n        <td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"custom_external_contact\") }}</td>\n        {%- else -%}\n         <td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"contact_mobile\") }}</td>\n        {%- endif -%}\n        </tr>\n    \n    </table>\n\n    <table class=\"col-xs-1\">\n        <tr><td></td></tr>\n    </table>\n\n    <table class=\"col-xs-5\">\n        <tr>\n            <td style=\"padding: 0px !important;\" >Date</td>\n            <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n            <td style=\"padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"transaction_date\") }}</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 0px !important;\">Valid Till</td>\n            <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n            <td style=\"padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"valid_till\") }}</td>\n        </tr>\n         {%- if doc.excel_team_type ==\"Printing Team\" -%}\n\n<tr>\n    <td style=\"padding: 0px !important;\">Model</td>\n    <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n    <td style=\"padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"custom_model\") }}</td>\n</tr>\n<tr>\n    <td style=\"padding: 0px !important;\">Product SN No</td>\n    <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n    <td style=\"padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"excel_product_sn_no\") }}</td>\n</tr>\n<tr>\n    <td style=\"padding: 0px !important;\">Machine Status</td>\n    <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n    <td style=\"padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"excel_machine_status\") }}</td>\n</tr>\n<tr>\n    <td style=\"padding: 0px !important;\">Reporting Engineer</td>\n    <td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\n    <td style=\"padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"excel_reporting_engineer\") }}</td>\n</tr>\n{%- endif -%}\n                                \n        </table>\n\n        <table class=\"col-xs-1\">\n            <tr><td></td></tr>\n        </table>\n\n        <table class=\"col-xs-6\">\n            \n            \n        </table>\n    </div>\n    \n\n\n<table class=\"table table-bordered\">\n    <tr>\n        <th class=\"text-left\">Sl.</th>\n        <!--<th class=\"text-center\">Item Code</th>-->\n        <th class=\"text-center\">Name</th>\n        <th class=\"text-center\">Qty</th>\n        <th class=\"text-center\">Rate</th>\n       \n        <th colspan='2' class=\"text-center\">Amount</th>\n\n    </tr>\n\n        {%- for row in doc.items -%}\n        <tr style=\"font-size: 10px;\">\n            <td class=\"text-left\">{{ row.idx }}</td>\n            <!--<td class=\"text-left\">{{ row.item_code }}</td>-->\n             {%- if row.item_code == 'EXCEL EXTERNAL ITEM' -%}\n    <td style=\"padding: 0px !important;\">{{ row.description }}</td>\n    {%- else -%}\n     <td style=\"padding: 0px !important;\">{{ row.item_code }}</td>\n    {%- endif -%}\n            <td class=\"text-right\">{{ row.qty }}</td>\n            <td class=\"text-right\">{{ row.rate }}</td>\n            \n            <td colspan='2' class=\"text-right\"><b>{{ row.amount }}</b> </td>\n        </tr>\n        \n        {%- endfor -%}\n\n{%- if doc.total_qty -%}\n<tr>\n    <td colspan=\"2\" class=\"text-right\" style=\"font-size: 12px; padding: 6px 8px 6px 30px !important;\"><b>Total Qty</td>\n    <td colspan=\"1\" class=\"text-right\" style=\"font-size: 12px; padding: 6px !important;\"><b>{{ doc.get_formatted(\"total_qty\") }}</td>\n    <td colspan=\"3\" class=\"text-right\" style=\"font-size: 12px; padding: 6px !important;\"></td>\n</tr>\n{% else %}\n{%- endif %}\n\n{%- if doc.discount_amount -%}\n<tr>\n    <td colspan=\"5\" class=\"text-right\" style=\"font-size: 12px; padding: 6px 8px 6px 30px !important;\"><b>Discount</b></td>\n    <td class=\"text-right\" style=\"font-size: 12px; padding: 6px !important;\"><b>{{ doc.get_formatted(\"discount_amount\") }}</b></td>\n</tr>\n{% else %}\n{%- endif %}\n\n{%- if doc.total_taxes_and_charges -%}\n<tr>\n    <td colspan=\"5\" class=\"text-right\" style=\"font-size: 12px; padding: 6px 8px 6px 30px !important;\"><b>VAT</td>\n    <td class=\"text-right\" style=\"font-size: 12px; padding: 6px !important;\"><b>{{ doc.get_formatted(\"total_taxes_and_charges\") }}</td>\n</tr>\n{% else %}\n{%- endif %}\n           \n<tr>\n    <td colspan=\"5\" class=\"text-right\" style=\"font-size: 12px; padding: 6px 8px 6px 30px !important;\"><b>Grand Total</b></td>\n    <td class=\"text-right\" style=\"font-size: 12px; padding: 6px !important;\"><b>{{ doc.get_formatted(\"grand_total\") }}</b></td>\n</tr>\n</table>\n\n<div>\n    {%- if doc.custom_remarks -%}\n        <p class=\"text-left\">Remarks: {{ doc.custom_remarks }}</p>\n    {% else %}\n    {%- endif %}\n</div>\n\n<div>\n    {%- if doc.terms -%}\n        <p class=\"text-left\">Terms & Conditions Details: {{ doc.terms }}</p>\n    {% else %}\n    {%- endif %}\n</div>\n\n<footer>\n\n    <div id=\"footer-html\" class=\"visible-pdf\" style=\"display: block !important; order: 1;\">\n\n        <div class=\"letter-head-footer\">\n            <p style=\"text-align: left; display: inline-block; width: 49.70%;\"><b style=\"border-top: 1px solid #000;\">Received with good condition by</b></p>\n            <p style=\"text-align: right; display: inline-block; width: 49.70%;\"><b style=\"border-top: 1px solid #000;\">For Excel Technologies Ltd.</b></p>\n\n            <div>\n                <img src=\"https://bd.bazrabd.org/files/SINV%20Futter%20all%20use%20final-01.jpg\" style=\"width: 861px;\">\n            </div>\n        </div>\n\n        <p class=\"text-center small page-number visible-pdf\">\n            Page <span class=\"page\"></span> of <span class=\"topage\"></span>\n        </p>\n    </div>\n\n</footer>",
